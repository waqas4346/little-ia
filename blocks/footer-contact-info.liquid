
{% liquid
  assign block_settings = block.settings
%}

<div
  class="footer-contact-info spacing-style"
  style="{% render 'spacing-style', settings: block_settings %}"
  {{ block.shopify_attributes }}
>
  
  {% if block_settings.text != blank %}
    <div class="footer-contact-info__text {{ block_settings.text_preset }}">
      {{ block_settings.text }}
    </div>
  {% endif %}

  {% if block_settings.email != blank %}
    <div class="footer-contact-info__email">
      <a href="mailto:{{ block_settings.email }}">
        {% if block_settings.email_icon != blank %}
          <img src="{{ block_settings.email_icon | image_url: width: 40 }}" alt="Email" width="20" height="20" loading="lazy">
        {% endif %}
        <span>{{ block_settings.email }}</span>
      </a>
    </div>
  {% endif %}

  {% if block_settings.phone != blank %}
    <div class="footer-contact-info__phone">
      <a href="tel:{{ block_settings.phone }}">
        {% if block_settings.whatsapp_icon != blank %}
          <img src="{{ block_settings.whatsapp_icon | image_url: width: 40 }}" alt="WhatsApp" width="20" height="20" loading="lazy">
        {% endif %}
        <span>Whatsapp - {{ block_settings.phone }}</span>
      </a>
    </div>
  {% endif %}

  <div class="social-icons-wrapper">
    {% if block_settings.text_2 != blank %}
      <div class="footer-contact-info__text-2 {{ block_settings.text_2_preset }}">
        {{ block_settings.text_2 }}
      </div>
    {% endif %}

    {% if block_settings.show_social_icons %}
      <div class="footer-contact-info__social">
        {% liquid
          assign social_links = 'facebook_url,instagram_url,youtube_url,tiktok_url,twitter_url,threads_url,linkedin_url,bluesky_url,snapchat_url,pinterest_url,tumblr_url,vimeo_url,custom_url' | split: ','
        %}

        {% for link_key in social_links %}
          {% liquid
            assign link_url = block_settings[link_key]
            assign render_icon = true

            if link_url != blank
              case link_key
                when 'facebook_url'
                  assign platform = 'facebook'
                when 'instagram_url'
                  assign platform = 'instagram'
                when 'youtube_url'
                  assign platform = 'youtube'
                when 'tiktok_url'
                  assign platform = 'tiktok'
                when 'twitter_url'
                  assign platform = 'twitter'
                when 'threads_url'
                  assign platform = 'threads'
                when 'linkedin_url'
                  assign platform = 'linkedin'
                when 'bluesky_url'
                  assign platform = 'bluesky'
                when 'snapchat_url'
                  assign platform = 'snapchat'
                when 'pinterest_url'
                  assign platform = 'pinterest'
                when 'tumblr_url'
                  assign platform = 'tumblr'
                when 'vimeo_url'
                  assign platform = 'vimeo'
                when 'custom_url'
                  assign platform = link_url | split: '//' | last | remove: 'www.' | split: '.' | first
                  case platform
                    when 'x'
                      assign platform = 'twitter'
                    else
                      assign render_icon = false
                  endcase
              endcase

              assign url_parts = link_url | split: '//' | last | split: '/'
              assign has_profile = false

              if url_parts.size > 1 and url_parts[1] != ''
                assign has_profile = true
              endif

              unless render_icon
                assign has_profile = true
              endunless
            endif
          %}

          {% if link_url != blank %}
            {% if request.design_mode or has_profile %}
              <div
                class="social-icons__icon-wrapper{% unless has_profile %} social-icons__icon-wrapper--disabled{% endunless %}"
              >
                <a
                  href="{{ link_url }}"
                  target="_blank"
                  rel="noopener noreferrer"
                  aria-label="{{ platform | capitalize }}"
                  {% unless has_profile %}
                    {% if request.design_mode %}
                      onclick="return false;"
                      aria-disabled="true"
                    {% endif %}
                  {% endunless %}
                >
                  <span class="social-icons__icon-label">{{ platform | capitalize }}</span>
                  {% if render_icon %}
                    {% case platform %}
                      {%- when 'facebook' -%}
                        <svg width="28" height="28" viewBox="0 0 28 28" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                        <rect width="28" height="28" fill="url(#pattern0_860_7626)"/>
                        <defs>
                        <pattern id="pattern0_860_7626" patternContentUnits="objectBoundingBox" width="1" height="1">
                        <use xlink:href="#image0_860_7626" transform="scale(0.01)"/>
                        </pattern>
                        <image id="image0_860_7626" width="100" height="100" preserveAspectRatio="none" xlink:href="data:image/png;base64,"/>
                        </defs>
                        </svg>
                      {%- when 'instagram' -%}
                        <svg width="28" height="28" viewBox="0 0 28 28" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                        <rect width="28" height="28" fill="url(#pattern0_860_7627)"/>
                        <defs>
                        <pattern id="pattern0_860_7627" patternContentUnits="objectBoundingBox" width="1" height="1">
                        <use xlink:href="#image0_860_7627" transform="scale(0.01)"/>
                        </pattern>
                        <image id="image0_860_7627" width="100" height="100" preserveAspectRatio="none" xlink:href="data:image/png;base64,"/>
                        </defs>
                        </svg>
                      {%- when 'youtube' -%}
                        <svg width="28" height="28" viewBox="0 0 28 28" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                        <rect width="28" height="28" fill="url(#pattern0_860_7630)"/>
                        <defs>
                        <pattern id="pattern0_860_7630" patternContentUnits="objectBoundingBox" width="1" height="1">
                        <use xlink:href="#image0_860_7630" transform="scale(0.01)"/>
                        </pattern>
                        <image id="image0_860_7630" width="100" height="100" preserveAspectRatio="none" xlink:href="data:image/png;base64,"/>
                        </defs>
                        </svg>
                      {%- else -%}
                        <svg
                          class="social-icons__icon icon-default"
                          aria-hidden="true"
                          focusable="false"
                          xmlns="http://www.w3.org/2000/svg"
                          viewBox="0 0 20 20"
                        >
                          {%- render 'icon', icon: platform -%}
                        </svg>
                    {%- endcase -%}
                  {% endif %}
                </a>
              </div>
            {% endif %}
          {% endif %}
        {% endfor %}
      </div>
    {% endif %}
    </div>

  {% if block_settings.show_payment_icons %}
    <div
      class="footer-contact-info__payment"
      style="--icon-gap: {{ block_settings.payment_gap }}px; --alignment: {{ block_settings.payment_alignment }};"
    >
      <span class="visually-hidden">{{ 'blocks.payment_methods' | t }}</span>
      <ul class="payment-icons__list" role="list">
        {%- for type in shop.enabled_payment_types -%}
          <li class="payment-icons__item">
            {{ type | payment_type_svg_tag: class: 'icon icon--full-color' }}
          </li>
        {%- endfor -%}
      </ul>
    </div>
  {% endif %}
</div>

{% stylesheet %}
  .footer-contact-info {
    display: flex;
    flex-direction: column;
    gap: var(--gap-sm, 12px);
  }

  .footer-contact-info__text,
  .footer-contact-info__text-2 {
    display: block;
    margin: 0;
  }

  .footer-contact-info__social {
    margin-bottom: 29px;
  }

  footer .footer-contact-info__email a,
  .footer-content .footer-contact-info__email a,
  footer .footer-contact-info__phone a,
  .footer-content .footer-contact-info__phone a {
    font-weight: 400;
    font-size: 14px;
    line-height: 140%;
    color: inherit;
    text-decoration: none;
    display: inline-flex;
    align-items: center;
    gap: 8px;
    vertical-align: middle;
  }

  footer .footer-contact-info__email a img,
  .footer-content .footer-contact-info__email a img,
  footer .footer-contact-info__phone a img,
  .footer-content .footer-contact-info__phone a img {
    display: block;
    width: 20px;
    height: 20px;
    max-width: 20px;
    max-height: 20px;
    object-fit: contain;
    flex-shrink: 0;
    vertical-align: middle;
  }

  footer .footer-contact-info__email a:hover,
  .footer-content .footer-contact-info__email a:hover,
  footer .footer-contact-info__phone a:hover,
  .footer-content .footer-contact-info__phone a:hover {
    color: #6688AE;
    text-decoration: underline;
  }

  .footer-contact-info__social {
    display: flex;
    gap: var(--gap-sm);
    flex-wrap: wrap;
  }

  .footer-contact-info__social .social-icons__icon-wrapper {
    display: flex;
    align-items: center;
    justify-content: center;
    height: var(--icon-size-lg);
  }

  .footer-contact-info__social .social-icons__icon {
    display: flex;
    flex-shrink: 0;
    width: var(--icon-size-lg);
    height: var(--icon-size-lg);
  }

  .footer-contact-info__social .social-icons__icon {
    display: none;
  }

  .footer-contact-info__social .social-icons__icon-wrapper:has(.social-icons__icon path) {
    width: var(--icon-size-lg);
  }

  .footer-contact-info__social .social-icons__icon-wrapper:has(.social-icons__icon path) .social-icons__icon {
    display: block;
  }

  .footer-contact-info__social .social-icons__icon-wrapper:has(svg) .social-icons__icon-label,
  .footer-contact-info__social .social-icons__icon-wrapper:has(.social-icons__icon path) .social-icons__icon-label {
    display: none;
  }

  .footer-contact-info__payment {
    width: 100%;
  }

  .footer-contact-info__payment .payment-icons__list {
    display: flex;
    align-items: center;
    justify-content: var(--alignment);
    flex-wrap: wrap;
    gap: var(--icon-gap);
    margin: 0;
    padding: 0;
  }

  .footer-contact-info__payment .payment-icons__item {
    display: flex;
    align-items: center;
  }

  @media screen and (max-width: 749px) {
    .footer-contact-info {
      gap: 0;
    }
    .footer-contact-info__payment {
      margin-bottom: 15px;
    }

    .footer-contact-info__email {
      margin-bottom: 14px;
    }

    .footer-contact-info__payment .payment-icons__list {
      justify-content: center;
    }

    .footer-contact-info__text,
    .footer-contact-info__text-2 {
      padding: 20px 0 !important;
    }

    .social-icons-wrapper {
      display: flex;
      align-items: center;
      gap: 14px;

      .footer-contact-info__text-2 p,
      .footer-contact-info__social {
        margin: 0;
      }
    }
  }

  footer .footer-contact-info__text,
  .footer-content .footer-contact-info__text,
  footer .footer-contact-info__text-2,
  .footer-content .footer-contact-info__text-2 {
    font-weight: 500;
    font-size: 18px;
    line-height: 26px;
    text-transform: uppercase;
    padding-block: var(--padding-sm);
    margin: 0;
  }

  /* Desktop: Hide original Contact Us section (we show desktop-top version instead) */
  @media screen and (min-width: 750px) {
    .footer-contact-info > .footer-contact-info__text.mobile\:hidden:first-of-type,
    .footer-contact-info > .footer-contact-info__email.mobile\:hidden,
    .footer-contact-info > .footer-contact-info__phone.mobile\:hidden {
      display: none;
    }

    .footer-contact-info__phone {
      margin-bottom: 15px;
    }
  }
{% endstylesheet %}


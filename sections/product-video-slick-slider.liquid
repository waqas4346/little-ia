             jQuery('#' + carouselId).find('.slick-slide').each(function() {
            jQuery(this).css({
              'width': '280px',
              'max-width': '280px',
              'min-width': '280px'
            });
          });
          }
          
          // Reapply gap on resize
          applyGap();
        }, 250);
      });
    }
    
    // Make only the play button clickable to start video
    const videoWrappers = carousel.querySelectorAll('.product-video-slick-slider__video-wrapper');
    videoWrappers.forEach(function(wrapper) {
      const deferredMedia = wrapper.querySelector('deferred-media');
      if (!deferredMedia) return;
      
      // Remove pointer cursor from wrapper - only play button should be clickable
      wrapper.style.cursor = 'default';
      
      // Find the play button and ensure it's the only clickable element
      function setupPlayButtonHandler() {
        const playButton = wrapper.querySelector('.deferred-media__poster-button');
        if (!playButton) return;
        
        // Remove any existing click handlers from wrapper
        wrapper.onclick = null;
        
        // Only allow play button to trigger video
        playButton.addEventListener('click', function(e) {
          e.stopPropagation();
          
          if (!deferredMedia.hasAttribute('data-media-loaded')) {
            // If media not loaded, let the default deferred-media behavior handle it
            return;
          }
          
          // Toggle play/pause when play button is clicked
          if (deferredMedia.toggleMedia && typeof deferredMedia.toggleMedia === 'function') {
            deferredMedia.toggleMedia();
          } else {
            // Fallback: try to play the video
            const videoElement = deferredMedia.querySelector('video');
            const iframe = deferredMedia.querySelector('iframe[data-video-type]');
            
            if (videoElement) {
              if (videoElement.paused) {
                videoElement.play().catch(function(err) {
                  console.log('Video play failed:', err);
                });
              } else {
                videoElement.pause();
              }
            } else if (iframe) {
              const videoType = iframe.dataset.videoType;
              if (videoType === 'youtube') {
                iframe.contentWindow?.postMessage('{"event":"command","func":"playVideo","args":""}', '*');
              } else if (videoType === 'vimeo') {
                iframe.contentWindow?.postMessage('{"method":"play"}', '*');
              }
            }
          }
        }, true);
        
        // Ensure play button has pointer cursor
        playButton.style.cursor = 'pointer';
      }
      
      // Setup play button handler when media is loaded
      const mediaObserver = new MutationObserver(function(mutations) {
        mutations.forEach(function(mutation) {
          if (mutation.type === 'attributes' && mutation.attributeName === 'data-media-loaded') {
            setupPlayButtonHandler();
          }
        });
      });
      
      mediaObserver.observe(deferredMedia, {
        attributes: true,
        attributeFilter: ['data-media-loaded']
      });
      
      // Setup immediately if media is already loaded
      if (deferredMedia.hasAttribute('data-media-loaded')) {
        setupPlayButtonHandler();
      } else {
        // Also try to setup after a short delay in case button exists before media loads
        setTimeout(setupPlayButtonHandler, 100);
      }
    });
  });
</script>


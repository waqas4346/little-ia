{% liquid
  assign heading = section.settings.heading
  assign subheading = section.settings.subheading
%}

<div class="section-background color-{{ section.settings.color_scheme }}"></div>
<div
  class="
    section
    section--{{ section.settings.section_width }}
    color-{{ section.settings.color_scheme }}
    spacing-style
    product-video-slick-slider-section
  "
  style="
    {% render 'spacing-style', settings: section.settings %}
  "
>
  <div class="product-video-slick-slider__header">
    {% if heading != blank %}
      <h2 class="product-video-slick-slider__heading">{{ heading }}</h2>
    {% endif %}
    {% if subheading != blank %}
      <h3 class="product-video-slick-slider__subheading">{{ subheading }}</h3>
    {% endif %}
  </div>

  {% if section.blocks.size > 0 %}
    <div class="product-video-slick-slider__wrapper">
      <div class="product-video-slick-slider__carousel" id="product-video-slick-slider-{{ section.id }}">
        {% for block in section.blocks %}
          {% liquid
            assign block_video = block.settings.video
            assign block_image = block.settings.image
            assign block_product = block.settings.product
            assign video_to_show = null
            assign preview_image = null

            if block_video != blank
              assign video_to_show = block_video
              assign preview_image = block_video.preview_image
            elsif block_image != blank
              assign video_to_show = block_image
              assign preview_image = block_image
            else
              assign preview_image = block_product.featured_image
            endif
          %}
          
          {% if block_product != blank %}
            <div class="product-video-slick-slider__item" {{ block.shopify_attributes }}>
              <div class="product-video-slick-slider__video-wrapper">
                {% if video_to_show != blank %}
                  {% render 'video',
                    video: video_to_show,
                    video_loop: true,
                    video_autoplay: false,
                    disable_controls: true,
                    section_id: section.id,
                    video_class: 'product-video-slick-slider__video',
                    video_preview_image: preview_image
                  %}
                {% elsif block_image != blank %}
                  <div class="product-video-slick-slider__image-wrapper">
                    {{
                      block_image
                      | image_url: width: 3840
                      | image_tag: widths: '240, 352, 832, 1200, 1600, 1920, 2560, 3840', sizes: '100vw', loading: 'lazy', class: 'product-video-slick-slider__image', alt: block_image.alt
                    }}
                  </div>
                {% else %}
                  <div class="product-video-slick-slider__placeholder-video">
                    {{ 'hero-apparel-3' | placeholder_svg_tag: 'product-video-slick-slider__placeholder-svg' }}
                  </div>
                {% endif %}
              </div>
              
              <div class="product-video-slick-slider__product-info">
                {% if block_product.featured_image != blank %}
                  <div class="product-video-slick-slider__product-image-wrapper">
                    <a href="{{ block_product.url }}" class="product-video-slick-slider__product-image-link">
                      {{
                        block_product.featured_image
                        | image_url: width: 200
                        | image_tag: widths: '100, 200', sizes: '100px', loading: 'lazy', class: 'product-video-slick-slider__product-image', alt: block_product.featured_image.alt
                      }}
                    </a>
                  </div>
                {% endif %}
                <div class="product-video-slick-slider__product-details">
                  <h4 class="product-video-slick-slider__product-title">
                    <a href="{{ block_product.url }}" class="product-video-slick-slider__product-link">
                      {{ block_product.title }}
                    </a>
                  </h4>
                  <div class="product-video-slick-slider__product-price">
                    {% render 'price', product_resource: block_product, show_sale_price_first: true %}
                  </div>
                </div>
                {% if settings.quick_add %}
                  <div class="product-video-slick-slider__quick-add-wrapper">
                    {% liquid
                      assign product_form_id = 'QuickAdd-ProductForm-' | append: block_product.id | append: '-' | append: block.id
                      assign variant = block_product.selected_or_first_available_variant
                    %}
                    <quick-add-component
                      class="quick-add color-{{ section.settings.color_scheme }}"
                      ref="quickAdd"
                      data-build-your-set="false"
                      data-product-title="{{ block_product.title }}"
                      data-product-url="{{ block_product.url }}"
                      data-product-handle="{{ block_product.handle }}"
                      data-quick-add-button="choose"
                      data-product-options-count="{{ block_product.options.size }}"
                    >
                      <a href="{{ block_product.url }}" class="product-video-slick-slider__product-url" ref="productCardLink" style="display: none;"></a>
                      <product-form-component
                        data-section-id="{{ section.id }}"
                        data-product-id="{{ block_product.id }}"
                        on:submit="/handleSubmit"
                        class="quick-add__product-form-component"
                      >
                        <div
                          class="visually-hidden"
                          aria-live="assertive"
                          role="status"
                          aria-atomic="true"
                          ref="liveRegion"
                        ></div>
                        {%- form 'product', block_product, id: product_form_id, novalidate: 'novalidate', data-type: 'add-to-cart-form' -%}
                          <input
                            type="hidden"
                            name="id"
                            ref="variantId"
                            value="{{ variant.id }}"
                            {% if variant.available == false %}
                              disabled
                            {% endif %}
                          >
                          <input
                            type="hidden"
                            name="quantity"
                            value="{% if variant.quantity_rule.min %}{{ variant.quantity_rule.min }}{% else %}1{% endif %}"
                          >
                          <button
                            class="button quick-add__button quick-add__button--choose add-to-cart-button product-video-slick-slider__plus-button"
                            type="button"
                            name="add"
                            on:click="quick-add-component/handleClick"
                          >
                            <span class="product-video-slick-slider__plus-text">+</span>
                          </button>
                        {%- endform -%}
                      </product-form-component>
                    </quick-add-component>
                  </div>
                {% endif %}
              </div>
            </div>
          {% endif %}
        {% endfor %}
      </div>
    </div>
  {% else %}
    <div class="product-video-slick-slider__placeholder">
      <p>Please add video items to display.</p>
    </div>
  {% endif %}
</div>

{% stylesheet %}
  /* Slick Slider Styles - Only for this section */
  .product-video-slick-slider-section .slick-slider {
    position: relative;
    display: block;
    box-sizing: border-box;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    -webkit-touch-callout: none;
    -khtml-user-select: none;
    -ms-touch-action: pan-x pan-y;
    touch-action: pan-x pan-y;
    -webkit-tap-highlight-color: transparent;
    margin: 0 auto;
  }

  .product-video-slick-slider-section .slick-list {
    position: relative;
    display: block;
    overflow: hidden;
    margin: 0 auto;
    padding: 0;
    -webkit-transform: translate3d(0, 0, 0);
    -moz-transform: translate3d(0, 0, 0);
    -ms-transform: translate3d(0, 0, 0);
    -o-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }

  .product-video-slick-slider-section .slick-list:focus {
    outline: none;
  }

  .product-video-slick-slider-section .slick-list.dragging {
    cursor: pointer;
    cursor: hand;
  }

  .product-video-slick-slider-section .slick-slider .slick-track,
  .product-video-slick-slider-section .slick-slider .slick-list {
    -webkit-transform: translate3d(0, 0, 0);
    -moz-transform: translate3d(0, 0, 0);
    -ms-transform: translate3d(0, 0, 0);
    -o-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }



  .product-video-slick-slider-section .slick-track {
    position: relative;
    top: 0;
    left: 0;
    display: flex;
    align-items: center;
    margin-left: auto;
    margin-right: auto;
  }

  .product-video-slick-slider-section .slick-track:before,
  .product-video-slick-slider-section .slick-track:after {
    display: table;
    content: '';
  }

  .product-video-slick-slider-section .slick-track:after {
    clear: both;
  }

  .product-video-slick-slider-section .slick-loading .slick-track {
    visibility: hidden;
  }

  .product-video-slick-slider-section .slick-slide {
    display: none;
    float: left;
    height: 100%;
    min-height: 1px;
  }

  .product-video-slick-slider-section .slick-slide img {
    display: block;
  }

  .product-video-slick-slider-section .slick-slide.slick-loading img {
    display: none;
  }

  .product-video-slick-slider-section .slick-slide.dragging img {
    pointer-events: none;
  }

  .product-video-slick-slider-section .slick-initialized .slick-slide {
    display: block;
  }

  /* Ensure consistent spacing between slides and fixed width */
  .product-video-slick-slider-section .slick-slide {
    width: 280px !important;
    margin-left: 12px !important;
    margin-right: 12px !important;
    box-sizing: border-box;
  }

  @media screen and (max-width: 749px) {
    /* Prevent horizontal scroll on mobile - hide overflow at section level */
    .product-video-slick-slider-section {
      overflow-x: hidden !important;
      overflow-y: visible !important;
      width: 100% !important;
      max-width: 100vw !important;
      position: relative;
    }
    
    /* Ensure parent section container also clips overflow */
    .product-video-slick-slider-section.section {
      overflow-x: hidden !important;
      width: 100% !important;
      max-width: 100vw !important;
    }
    
    /* On mobile, let Slick handle slide widths with centerMode - don't force fixed widths */
    .product-video-slick-slider-section .slick-slide {
      width: auto !important;
      margin-right: 0 !important;
    }
    
    /* Center slide should be fully visible */
    .product-video-slick-slider-section .slick-slide.slick-center {
      opacity: 1 !important;
      visibility: visible !important;
      pointer-events: auto !important;
      z-index: 2;
    }
    
    /* Show all slides on mobile with reduced opacity for non-center */
    .product-video-slick-slider-section .slick-slide:not(.slick-center) {
      opacity: 0.6 !important;
      visibility: visible !important;
      pointer-events: auto !important;
      z-index: 1;
    }
    
    /* Let Slick's centerPadding handle the spacing - clip overflow but allow partial views */
    .product-video-slick-slider-section .slick-list {
      padding: 0 !important;
      overflow: hidden !important;
      width: 100% !important;
      max-width: 100vw !important;
    }
    
    /* Ensure the carousel container clips overflow */
    .product-video-slick-slider-section .slick-slider {
      overflow: hidden !important;
      width: 100% !important;
      max-width: 100vw !important;
    }
    
    /* Wrapper should not cause overflow */
    .product-video-slick-slider__wrapper {
      overflow: hidden !important;
      width: 100% !important;
      max-width: 100vw !important;
    }
    
    /* Ensure the item inside takes full width with padding */
    .product-video-slick-slider-section .slick-slide.slick-center .product-video-slick-slider__item {
      width: 100% !important;
      max-width: 100% !important;
      box-sizing: border-box;
    }
    
    /* Non-center slides also need proper padding */
    .product-video-slick-slider-section .slick-slide:not(.slick-center) .product-video-slick-slider__item {
      box-sizing: border-box;
    }
    
    /* Make sure the track centers properly but doesn't cause overflow */
    .product-video-slick-slider-section .slick-track {
      display: flex !important;
      align-items: center;
      width: 100% !important;
    }
    
    /* Ensure carousel element doesn't exceed viewport */
    .product-video-slick-slider__carousel {
      width: 100% !important;
      max-width: 100vw !important;
      overflow: hidden !important;
    }
  }

  .product-video-slick-slider-section .slick-loading .slick-slide {
    visibility: hidden;
  }

  .product-video-slick-slider-section .slick-vertical .slick-slide {
    display: block;
    height: auto;
    border: 1px solid transparent;
  }

  .product-video-slick-slider-section .slick-arrow.slick-hidden {
    display: none;
  }

  /* Hide arrows */
  .product-video-slick-slider-section .slick-arrow {
    display: none !important;
  }

  /* Center Mode Styles - Center item is larger */
  .product-video-slick-slider-section .slick-center .product-video-slick-slider__item {
    transform: scale(1.02);
    opacity: 1;
    z-index: 2;
  }
  .product-video-slick-slider-section .slick-center .slick-slide > div {
    /* max-width: 328px !important; */
        width: 100% !important;
  }

  .product-video-slick-slider-section .slick-center {
        max-width: 328px !important;
        width: 100% !important;
  }

  .product-video-slick-slider-section .slick-slide:not(.slick-center) .product-video-slick-slider__item {
    opacity: 1;
    transform: scale(1, 0.9);
    transition: transform 0.4s ease;
  }
  
  /* Prevent product-info from stretching on non-center items */
  .product-video-slick-slider-section .slick-slide:not(.slick-center) .product-video-slick-slider__product-info {
    transform: scale(1, 1.111); /* Inverse of 0.9 to maintain original proportions: 1/0.9 = 1.111 */
    transform-origin: center;
  }

  /* Header Styles - Matching video slider */
  .product-video-slick-slider__header {
    text-align: center;
    margin-bottom: 16px;
  }

  .product-video-slick-slider__heading {
    margin: 0;
    font-family: 'Sacramento', cursive, sans-serif;
    font-weight: 400;
    font-size: 30px;
    line-height: 100%;
    color: #7295BB;
    text-align: center;
    text-transform: lowercase;
  }

  .product-video-slick-slider__subheading {
    margin: 0;
    font-weight: 500;
    font-size: 24px;
    line-height: 150%;
    text-transform: uppercase;
    color: #1D425A;
    text-align: center;
  }

  @media screen and (min-width: 750px) {
    .product-video-slick-slider__header {
      margin-bottom: 24px;
    }

    .product-video-slick-slider__heading {
      font-size: 38px;
    }
    .product-video-slick-slider__subheading {
      font-size: 32px;
    }
  }

  /* Wrapper Styles */
  .product-video-slick-slider__wrapper {
    width: 100%;
    position: relative;
    padding: 0;
    display: flex;
    justify-content: center;
  }

  @media screen and (max-width: 749px) {
    .product-video-slick-slider__wrapper {
      padding: 0;
      overflow: hidden !important;
      width: 100% !important;
      max-width: 100vw !important;
    }
  }
  
  /* Ensure carousel is centered */
  .product-video-slick-slider__carousel {
    width: 100%;
    max-width: 100%;
  }
  
  /* Center the slick track on larger screens */
  @media screen and (min-width: 750px) {
    .product-video-slick-slider-section .slick-track {
      display: flex;
      align-items: center;
      margin-left: auto;
      margin-right: auto;
    }
    
    .product-video-slick-slider-section .slick-slider {
      display: flex;
      justify-content: center;
    }
  }

  /* Item Styles */
  .product-video-slick-slider__item {
    width: 100% !important;
    max-width: 100% !important;
    height: 100%;
    display: flex;
    flex-direction: column;
    padding: 0;
    margin: 0;
    box-sizing: border-box;
    transition: transform 0.3s ease, opacity 0.3s ease;
  }

  .product-video-slick-slider__image-wrapper {
    width: 100% !important;
    max-width: 100% !important;

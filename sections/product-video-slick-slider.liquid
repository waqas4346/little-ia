  }
  
  .product-video-slick-slider__quick-add-wrapper .quick-add__button--choose:hover:not(:disabled) .product-video-slick-slider__plus-text,
  .product-video-slick-slider__quick-add-wrapper .button.quick-add__button--choose:hover:not(:disabled) .product-video-slick-slider__plus-text,
  .product-video-slick-slider__quick-add-wrapper .add-to-cart-button.quick-add__button--choose:hover:not(:disabled) .product-video-slick-slider__plus-text,
  .product-video-slick-slider__quick-add-wrapper .quick-add__button.add-to-cart-button:hover:not(:disabled) .product-video-slick-slider__plus-text,

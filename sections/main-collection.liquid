

<script
  src="{{ 'results-list.js' | asset_url }}"
  type="module"
  fetchpriority="low"
></script>
<script
  src="{{ 'dialog.js' | asset_url }}"
  type="module"
></script>

{% javascript %}
  const url = new URL(window.location.href);
  if (url.hash) {
    document.addEventListener(
      'DOMContentLoaded',
      () => {
        const card = document.getElementById(url.hash.slice(1));
        if (card) {
          card.scrollIntoView({ behavior: 'instant' });
        }
      },
      { once: true }
    );
  }

  // Function to check if there are active filters
  function hasActiveFilters() {
    const urlParams = new URLSearchParams(window.location.search);
    return Array.from(urlParams.keys()).some(key => key.startsWith('filter.'));
  }

  // Function to check if filters are manually hidden
  function areFiltersManuallyHidden() {
    const resultsList = document.querySelector('results-list');
    return resultsList?.dataset.filtersManuallyHidden === 'true';
  }

  // Function to set manually hidden state
  function setFiltersManuallyHidden(hidden) {
    const resultsList = document.querySelector('results-list');
    if (resultsList) {
      if (hidden) {
        resultsList.dataset.filtersManuallyHidden = 'true';
      } else {
        delete resultsList.dataset.filtersManuallyHidden;
      }
    }
  }

  // Function to check and show filters if active (only if not manually hidden and on desktop)
  function checkAndShowFilters() {
    const resultsList = document.querySelector('results-list');
    if (!resultsList) return;
    
    // Only show filters on desktop (screen width >= 750px)
    const isDesktop = window.innerWidth >= 750;
    
    // If there are active filters and they haven't been manually hidden, show the filters component (desktop only)
    if (hasActiveFilters() && !areFiltersManuallyHidden() && isDesktop) {
      resultsList.classList.add('filters-visible');
    } else if (!isDesktop) {
      // Remove filters-visible class on mobile
      resultsList.classList.remove('filters-visible');
    }
  }

  // Update products count with pagination info
  function updateProductsCount() {
    const productsCountWrapper = document.querySelector('.collection-results-count-wrapper[data-total-products]');
    if (!productsCountWrapper) return;

    const productGrid = document.querySelector('.product-grid');
    if (!productGrid) return;

    const totalProducts = parseInt(productsCountWrapper.dataset.totalProducts) || 0;
    const productItems = productGrid.querySelectorAll('.product-grid__item');
    const currentCount = productItems.length;

    if (currentCount === 0) return;

    // Get page number from URL or from first product item
    const urlParams = new URLSearchParams(window.location.search);
    let currentPage = parseInt(urlParams.get('page')) || 1;
    
    // Try to get page from product items
    const firstProduct = productItems[0];
    if (firstProduct && firstProduct.dataset.page) {
      currentPage = parseInt(firstProduct.dataset.page) || 1;
    }
    
    // Default page size is 24
    const pageSize = 24;
    
    // Calculate range
    const startNumber = (currentPage - 1) * pageSize + 1;
    let endNumber = currentPage * pageSize;
    
    // Cap at total products
    if (endNumber > totalProducts) {
      endNumber = totalProducts;
    }
    
    // If we have fewer items than expected, adjust
    if (currentCount < endNumber - startNumber + 1) {
      endNumber = startNumber + currentCount - 1;
    }

    const countText = productsCountWrapper.querySelector('.collection-results-count-text');
    if (countText) {
      countText.textContent = `Showing ${startNumber}-${endNumber} of ${totalProducts} products`;
    }
  }

  // Toggle filters visibility
  document.addEventListener('DOMContentLoaded', () => {
    // Update products count on load
    updateProductsCount();
    
    // Update on filter changes
    document.addEventListener('filter:update', () => {
      setTimeout(updateProductsCount, 100);
    });
    
    // Handle sort option selection - update styling immediately
    const sortOptions = document.querySelectorAll('.mobile-sort-modal__option');
    sortOptions.forEach(option => {
      const radio = option.querySelector('.mobile-sort-modal__radio');
      const text = option.querySelector('.mobile-sort-modal__option-text');
      
      if (radio && text) {
        // Handle click on the label
        option.addEventListener('click', (e) => {
          // Don't prevent default if clicking directly on the radio
          if (e.target === radio) return;
          
          // Uncheck all radios
          document.querySelectorAll('.mobile-sort-modal__radio').forEach(r => {
            r.checked = false;
          });
          
          // Remove selected class from all options
          document.querySelectorAll('.mobile-sort-modal__option-text').forEach(t => {
            t.classList.remove('mobile-sort-modal__option-text--selected');
          });
          
          // Check the clicked radio
          radio.checked = true;
          text.classList.add('mobile-sort-modal__option-text--selected');
        });
        
        // Handle radio change event
        radio.addEventListener('change', () => {
          if (radio.checked) {
            // Remove selected class from all options
            document.querySelectorAll('.mobile-sort-modal__option-text').forEach(t => {
              t.classList.remove('mobile-sort-modal__option-text--selected');
            });
            // Add selected class to this option
            text.classList.add('mobile-sort-modal__option-text--selected');
          }
        });
      }
    });
    
    // Handle mobile sort modal Apply button
    const mobileSortApply = document.getElementById('mobile-sort-apply');
    if (mobileSortApply) {
      mobileSortApply.addEventListener('click', () => {
        const form = document.getElementById('mobile-sort-form');
        if (form) {
          form.submit();
        }
      });
    }
    
    // Handle mobile sort modal Reset button
    const mobileSortReset = document.getElementById('mobile-sort-reset');
    if (mobileSortReset) {
      mobileSortReset.addEventListener('click', () => {
        const form = document.getElementById('mobile-sort-form');
        if (form) {
          // Build URL with current filters but without sort_by
          const url = new URL(form.action);
          const currentParams = new URLSearchParams(window.location.search);
          
          // Remove sort_by but keep all filter parameters
          currentParams.delete('sort_by');
          
          // Update URL with preserved filters
          const newUrl = new URL(form.action);
          newUrl.search = currentParams.toString();
          window.location.href = newUrl.toString();
        }
      });
    }
    
    // Update sort value display when sort changes
    const updateSortDisplay = () => {
      const sortValueDisplay = document.querySelector('[data-sort-value]');
      if (!sortValueDisplay) return;
      
      const urlParams = new URLSearchParams(window.location.search);
      const currentSort = urlParams.get('sort_by') || '';
      
      // Find the current sort name from the sorting component
      const sortingComponent = document.querySelector('sorting-filter-component');
      if (sortingComponent) {
        const statusValue = sortingComponent.querySelector('.sorting-status-value');
        if (statusValue) {
          sortValueDisplay.textContent = statusValue.textContent.trim();
        }
      }
    };
    
    // Update sort display on load and when filters update
    updateSortDisplay();
    document.addEventListener('filter:update', () => {
      setTimeout(updateSortDisplay, 100);
    });
    
    // Update filter count display
    const updateFilterDisplay = () => {
      const filterButton = document.getElementById('mobile-filter-button');
      if (!filterButton) return;
      
      const filterValueDisplay = filterButton.querySelector('.collection-mobile-sticky-bar__value');
      if (!filterValueDisplay) return;
      
      const urlParams = new URLSearchParams(window.location.search);
      const activeFilterCount = Array.from(urlParams.keys()).filter(key => key.startsWith('filter.')).length;
      
      if (activeFilterCount > 0) {
        // Count actual active filter values
        let totalActiveValues = 0;
        urlParams.forEach((value, key) => {
          if (key.startsWith('filter.')) {
            // Count comma-separated values
            const values = value.split(',').filter(v => v.trim() !== '');
            totalActiveValues += values.length;
          }
        });
        filterValueDisplay.textContent = totalActiveValues > 0 ? `${totalActiveValues} Applied` : 'No Filter Applied';
      } else {
        filterValueDisplay.textContent = 'No Filter Applied';
      }
    };
    
    // Update filter display on load and when filters update
    updateFilterDisplay();
    document.addEventListener('filter:update', () => {
      setTimeout(updateFilterDisplay, 100);
    });
    
    // Handle active state for sticky bar buttons when modals open
    const mobileSortModal = document.getElementById('mobile-sort-modal');
    const sortButton = document.getElementById('mobile-sort-button');
    if (mobileSortModal && sortButton) {
      mobileSortModal.addEventListener('dialog:open', () => {
        sortButton.classList.add('collection-mobile-sticky-bar__button--active');
      });
      mobileSortModal.addEventListener('dialog:close', () => {
        sortButton.classList.remove('collection-mobile-sticky-bar__button--active');
      });
    }
    
    const resultsList = document.querySelector('results-list');
    
    if (resultsList) {
      // Check if there are active filters on page load
      checkAndShowFilters();
      
      // Use event delegation on document - only handle clicks on our specific buttons
      // This ensures filter clicks work normally since we're not attached to resultsList
      document.addEventListener('click', (event) => {
        // Only handle clicks on our specific buttons
        const filtersButton = event.target.closest('#collection-filters-toggle');
        const closeButton = event.target.closest('#collection-filters-close');
        
        if (filtersButton) {
          event.preventDefault();
          event.stopPropagation();
          const isVisible = resultsList.classList.contains('filters-visible');
          resultsList.classList.toggle('filters-visible');
          
          // Track manual hide state
          if (isVisible) {
            // User is hiding the filters
            setFiltersManuallyHidden(true);
          } else {
            // User is showing the filters
            setFiltersManuallyHidden(false);
          }
          return;
        }
        
        if (closeButton) {
          event.preventDefault();
          event.stopPropagation();
          // Close the filters sidebar
          resultsList.classList.remove('filters-visible');
          setFiltersManuallyHidden(true);
          return;
        }
        
        // For all other clicks (including filter inputs), do nothing - let them work normally
      });

      // Use MutationObserver to preserve filters-visible class during morph
      // But only if filters haven't been manually hidden and we're on desktop
      let isRestoring = false;
      const observer = new MutationObserver((mutations) => {
        if (isRestoring) return;
        
        const isDesktop = window.innerWidth >= 750;
        
        mutations.forEach((mutation) => {
          if (mutation.type === 'attributes' && mutation.attributeName === 'class') {
            const target = mutation.target;
            if (target === resultsList && hasActiveFilters() && !areFiltersManuallyHidden() && isDesktop) {
              // If filters-visible was removed but we have active filters and they're not manually hidden, restore it (desktop only)
              if (!target.classList.contains('filters-visible')) {
                isRestoring = true;
                // Use requestAnimationFrame to avoid conflicts with morph
                requestAnimationFrame(() => {
                  if (hasActiveFilters() && !areFiltersManuallyHidden() && window.innerWidth >= 750 && !target.classList.contains('filters-visible')) {
                    target.classList.add('filters-visible');
                  }
                  isRestoring = false;
                });
              }
            } else if (target === resultsList && !isDesktop) {
              // Remove filters-visible class on mobile
              if (target.classList.contains('filters-visible')) {
                target.classList.remove('filters-visible');
              }
            }
          }
        });
      });

      // Observe class changes on results-list
      observer.observe(resultsList, {
        attributes: true,
        attributeFilter: ['class']
      });
    }
  });
  
  // Listen for filter updates to keep filters visible after applying
  document.addEventListener('filter:update', () => {
    // When new filters are applied, reset the manually hidden state and show filters
    setFiltersManuallyHidden(false);
    
    // Mark that we should keep filters visible immediately (desktop only)
    const resultsList = document.querySelector('results-list');
    const isDesktop = window.innerWidth >= 750;
    if (resultsList && hasActiveFilters() && isDesktop) {
      resultsList.classList.add('filters-visible');
    } else if (resultsList && !isDesktop) {
      // Remove filters-visible class on mobile
      resultsList.classList.remove('filters-visible');
    }
    
    // Check after view transition completes (if using view transitions)
    if (document.startViewTransition) {
      // View transition is async, check after it completes
      setTimeout(() => {
        checkAndShowFilters();
      }, 100);
    }
    
    // Also check after morph completes - use requestAnimationFrame chain
    requestAnimationFrame(() => {
      requestAnimationFrame(() => {
        checkAndShowFilters();
      });
    });
    
    // Final check after a longer delay to catch any late morphs
    setTimeout(() => {
      checkAndShowFilters();
    }, 400);
  });
{% endjavascript %}

{% comment %} We always render this full-width, as the child blocks have width: page/full settings {% endcomment %}
<div class="section-background color-{{ section.settings.color_scheme }}"></div>
<results-list
  class="section product-grid-container color-{{ section.settings.color_scheme }}"
  style="--padding-block-start: {{ section.settings.padding-block-start }}px; --padding-block-end: {{ section.settings.padding-block-end }}px;"
  section-id="{{ section.id }}"
  infinite-scroll="{{ section.settings.enable_infinite_scroll }}"
>
  {% render 'skip-to-content-link', href: '#ResultsList', text: 'accessibility.skip_to_results_list' %}

  <div class="collection-filters-button-wrapper">
    <button type="button" class="collection-filters-button" id="collection-filters-toggle" aria-label="Filter & Sort">
      <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
<rect width="20" height="20" fill="url(#pattern0_860_10552)"/>
<defs>
<pattern id="pattern0_860_10552" patternContentUnits="objectBoundingBox" width="1" height="1">
<use xlink:href="#image0_860_10552" transform="scale(0.00195312)"/>
</pattern>
<image id="image0_860_10552" width="512" height="512" preserveAspectRatio="none" xlink:href="data:image/png;base64,"/>
</defs>
</svg>

      Filter & Sort
    </button>
  </div>

  <div
    class="collection-wrapper grid gap-style{% if section.settings.product_grid_width == 'full-width' %} collection-wrapper--grid-full-width{% endif %}"
  >
    {% content_for 'block',
      type: 'filters',
      id: 'filters',
      results: collection,
      results_size: collection.products_count
    %}

    {% assign products_per_page = 24 %}

    {% if section.settings.enable_infinite_scroll == false %}
      {% assign products_per_page = section.settings.products_per_page %}
    {% endif %}

    {% paginate collection.products by products_per_page %}
      {% capture children %}
        {% for product in collection.products %}
          <li
            id="{{ section.id }}-{{ product.id }}"
            class="product-grid__item product-grid__item--{{ forloop.index0 }}"
            data-page="{{ paginate.current_page }}"
            data-product-id="{{ product.id }}"
            ref="cards[]"
          >
            {% # theme-check-disable %}
            {% content_for 'block', type: '_product-card', id: 'product-card', closest.product: product %}
            {% # theme-check-enable %}
          </li>
        {% endfor %}
      {% endcapture %}

      {% render 'product-grid',
        section: section,
        children: children,
        products: collection.products,
        paginate: paginate,
        enable_infinite_scroll: section.settings.enable_infinite_scroll,
        collection: collection
      %}
    {% endpaginate %}
  </div>

  {%- comment -%} Mobile sticky bottom bar for Sort By and Filter {%- endcomment -%}
  {%- liquid
    assign sort_by = collection.sort_by | default: collection.default_sort_by
    assign current_sort_name = 'Featured'
    for option in collection.sort_options
      if option.value == sort_by
        assign current_sort_name = option.name
      endif
    endfor
    
    assign total_active_filters = 0
    for filter in collection.filters
      case filter.type
        when 'price_range'
          if filter.min_value.value != null or filter.max_value.value != null
            assign total_active_filters = total_active_filters | plus: 1
          endif
        when 'boolean'
          if filter.active_values[0].value
            assign total_active_filters = total_active_filters | plus: 1
          endif
        else
          assign active_value_count = filter.active_values | size
          assign total_active_filters = total_active_filters | plus: active_value_count
      endcase
    endfor
  -%}
  
  {%- comment -%} Mobile Sort By Modal {%- endcomment -%}
  <dialog-component id="mobile-sort-modal" class="mobile-sort-modal">
    <dialog
      ref="dialog"
      class="mobile-sort-modal__dialog dialog-modal dialog-drawer dialog-drawer--right"
      scroll-lock
      aria-labelledby="mobile-sort-modal-heading"
    >
      <div class="mobile-sort-modal__form-wrapper">
        <form id="mobile-sort-form" class="mobile-sort-modal__form" action="{{ collection.url }}" method="get">
          <div class="mobile-sort-modal__title-wrapper">
            <h2 id="mobile-sort-modal-heading" class="mobile-sort-modal__title h3">Sort By</h2>
            <button
              ref="closeButton"
              on:click="/closeDialog"
              class="button facets-drawer__close button-unstyled close-button"
              type="button"
              aria-label="Close"
            >
              <span class="svg-wrapper svg-wrapper--small" title="Close">
                {{- 'icon-close.svg' | inline_asset_content -}}
              </span>
            </button>
          </div>
          
          <div class="mobile-sort-modal__content">
            {%- comment -%} Preserve current filters {%- endcomment -%}
            {%- for filter in collection.filters -%}
              {%- for filter_value in filter.active_values -%}
                <input type="hidden" name="{{ filter.param_name }}" value="{{ filter_value.value }}">
              {%- endfor -%}
              {%- if filter.type == 'price_range' -%}
                {%- if filter.min_value.value -%}
                  <input type="hidden" name="{{ filter.min_value.param_name }}" value="{{ filter.min_value.value }}">
                {%- endif -%}
                {%- if filter.max_value.value -%}
                  <input type="hidden" name="{{ filter.max_value.param_name }}" value="{{ filter.max_value.value }}">
                {%- endif -%}
              {%- endif -%}
            {%- endfor -%}
            
            {% for option in collection.sort_options %}
              <label class="mobile-sort-modal__option">
                <input
                  type="radio"
                  name="sort_by"
                  value="{{ option.value }}"
                  {% if option.value == sort_by %}checked{% endif %}
                  class="mobile-sort-modal__radio"
                />
                <span class="mobile-sort-modal__option-text {% if option.value == sort_by %}mobile-sort-modal__option-text--selected{% endif %}">
                  {{ option.name }}
                </span>
              </label>
            {% endfor %}
          </div>
          
          <div class="mobile-sort-modal__actions">
            <button
              type="button"
              class="mobile-sort-modal__button mobile-sort-modal__button--apply"
              id="mobile-sort-apply"
            >
              Apply
            </button>
            <button
              type="button"
              class="mobile-sort-modal__button mobile-sort-modal__button--reset"
              id="mobile-sort-reset"
            >
              Reset
            </button>
          </div>
        </form>
      </div>
    </dialog>
  </dialog-component>
  
  <div class="collection-mobile-sticky-bar" data-mobile-sticky-bar>
    <button 
      type="button" 
      class="collection-mobile-sticky-bar__button collection-mobile-sticky-bar__button--sort"
      id="mobile-sort-button"
      aria-label="Sort By"
      on:click="#mobile-sort-modal/showDialog"
    >
      <svg width="26" height="26" viewBox="0 0 26 26" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
        <rect width="26" height="26" fill="url(#pattern0_860_13718)"/>
        <defs>
        <pattern id="pattern0_860_13718" patternContentUnits="objectBoundingBox" width="1" height="1">
        <use xlink:href="#image0_860_13718" transform="scale(0.00195312)"/>
        </pattern>
        <image id="image0_860_13718" width="512" height="512" preserveAspectRatio="none" xlink:href="data:image/png;base64,"/>
        </defs>
        </svg>

      <div class="collection-mobile-sticky-bar__content">
        <span class="collection-mobile-sticky-bar__label">Sort By</span>
        <span class="collection-mobile-sticky-bar__value" data-sort-value>{{ current_sort_name }}</span>
      </div>
    </button>
    <div class="collection-mobile-sticky-bar__divider"></div>
    <button 
      type="button" 
      class="collection-mobile-sticky-bar__button collection-mobile-sticky-bar__button--filter"
      id="mobile-filter-button"
      aria-label="Filter"
      on:click="#filters-drawer/showDialog"
    >
      <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
<rect width="20" height="20" fill="url(#pattern0_860_13724)"/>
<defs>
<pattern id="pattern0_860_13724" patternContentUnits="objectBoundingBox" width="1" height="1">
<use xlink:href="#image0_860_13724" transform="scale(0.00195312)"/>
</pattern>
<image id="image0_860_13724" width="512" height="512" preserveAspectRatio="none" xlink:href="data:image/png;base64,"/>
</defs>
</svg>

      <div class="collection-mobile-sticky-bar__content">
        <span class="collection-mobile-sticky-bar__label">Filter</span>
        <span class="collection-mobile-sticky-bar__value">
          {% if total_active_filters > 0 %}
            {{ total_active_filters }} Applied
          {% else %}
            No Filter Applied
          {% endif %}
        </span>
      </div>
    </button>
  </div>
</results-list>

{% stylesheet %}
  .collection-filters-button-wrapper {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    padding-block-start: 16px;
    padding-block-end: 16px;
  }

  /* Hide button when filters sidebar is open */
  .filters-visible .collection-filters-button-wrapper {
    @media screen and (min-width: 750px) {
      display: none;
    }
  }

  @media screen and (max-width: 749px) {
    .collection-filters-button-wrapper {
      display: none;
    }
  }

  .collection-filters-button {
    display: flex;
    gap: 6px;
    align-items: center;
    padding: 0;
    background-color: transparent;
    border: none;
    font-family: 'Outfit', sans-serif;
    font-style: normal;
    font-weight: 500;
    font-size: 16px;
    line-height: 140%;
    color: #222222;
    cursor: pointer;
    transition: all 0.2s ease;
  }

  .collection-filters-button__icon {
    margin-right: 7px;
    width: 16px;
    height: 16px;
    flex-shrink: 0;
  }


  .main-collection-grid {
    grid-column: var(--grid-column--mobile);

    @media screen and (min-width: 750px) {
      grid-column: var(--grid-column--desktop);
    }
  }

  .collection-wrapper {
    @media screen and (min-width: 750px) {
      grid-template-columns:
        1fr repeat(
          var(--centered-column-number),
          minmax(0, calc((var(--page-width) - var(--page-margin) * 2) / var(--centered-column-number)))
        )
        1fr;
    }
  }

  .collection-wrapper:has(.facets-block-wrapper--full-width),
  .collection-wrapper:has(.collection-wrapper--full-width) {
    @media screen and (min-width: 750px) {
      grid-column: 1 / -1;
      grid-template-columns:
        minmax(var(--page-margin), 1fr) repeat(
          var(--centered-column-number),
          minmax(0, calc((var(--page-width) - var(--page-margin) * 2) / var(--centered-column-number)))
        )
        minmax(var(--page-margin), 1fr);
    }
  }

  .collection-wrapper:has(.facets--vertical) .facets-block-wrapper--vertical:not(.hidden) ~ .main-collection-grid {
    @media screen and (min-width: 750px) {
      grid-column: var(--facets-vertical-col-width) / var(--full-width-column-number);
    }
  }

  .collection-wrapper:has(.facets-block-wrapper--vertical:not(#filters-drawer)):has(.collection-wrapper--full-width) {
    @media screen and (min-width: 750px) {
      grid-column: 1 / -1;
      grid-template-columns: 0fr repeat(var(--centered-column-number), minmax(0, 1fr)) 0fr;
    }
  }

  :is(.collection-wrapper--full-width, .collection-wrapper--full-width-on-mobile)
    [product-grid-view='default']
    .product-grid__card {
    @media screen and (max-width: 749px) {
      padding-inline-start: max(var(--padding-xs), var(--padding-inline-start));
      padding-inline-end: max(var(--padding-xs), var(--padding-inline-end));
    }
  }

  :is(.collection-wrapper--full-width, .collection-wrapper--full-width-on-mobile)
    [product-grid-view='mobile-single']
    .product-grid__card {
    @media screen and (max-width: 749px) {
      padding-inline-start: max(var(--padding-xs), var(--padding-inline-start));
      padding-inline-end: max(var(--padding-xs), var(--padding-inline-end));
    }
  }

  /* Make product media go edge-to-edge by using negative margins */
  :is(.collection-wrapper--full-width) .card-gallery,
  :is(.collection-wrapper--full-width-on-mobile) .card-gallery {
    @media screen and (max-width: 749px) {
      margin-inline-start: calc(-1 * max(var(--padding-xs), var(--padding-inline-start)));
      margin-inline-end: calc(-1 * max(var(--padding-xs), var(--padding-inline-end)));
    }
  }

  /* Hide filter component by default */
  .facets-block-wrapper:not(#filters-drawer),
  .facets-controls-wrapper {
    display: none !important;
  }

  /* Show filter component when filters-visible class is added (desktop only) */
  .filters-visible .facets-block-wrapper:not(#filters-drawer) {
    @media screen and (min-width: 750px) {
      display: block !important;
    }
  }

  .filters-visible .facets-controls-wrapper {
    @media screen and (min-width: 750px) {
      display: flex !important;
    }
  }

  /* Keep filters hidden on mobile even when filters-visible class is present */
  @media screen and (max-width: 749px) {
    .filters-visible .facets-block-wrapper:not(#filters-drawer),
    .filters-visible .facets-controls-wrapper {
      display: none !important;
    }
  }

  /* Make main-collection-grid full width when filters are hidden */
  .collection-wrapper .main-collection-grid {
    @media screen and (min-width: 750px) {
      grid-column: 1 / -1 !important;
    }
  }

  /* When filters are visible, ensure they use the existing grid layout */
  .filters-visible .collection-wrapper .facets-controls-wrapper {
    @media screen and (min-width: 750px) {
      grid-column: 1 / -1;
    }
  }

  .filters-visible .collection-wrapper .facets-block-wrapper--vertical:not(#filters-drawer) {
    @media screen and (min-width: 750px) {
      display: block !important;
      grid-column: 2 / var(--facets-vertical-col-width);
      width: 282px;
      max-width: 282px;
      min-width: 282px;
    }
  }

  /* Position main-collection-grid next to filters when filters are visible */
  .filters-visible .collection-wrapper .main-collection-grid {
    @media screen and (min-width: 750px) {
      grid-column: var(--facets-vertical-col-width) / -1 !important;
    }
  }

  /* Set product card and image to 324px on desktop */
  @media screen and (min-width: 750px) {
    /* Ensure product grid items are exactly 324px */
    .product-grid__item {
      width: 324px;
    }

    /* Set product image container to 324px x 324px */
    .product-grid .card-gallery .product-media-container {
      width: 324px;
      height: 324px;
    }

    .product-grid .card-gallery .product-media-container img,
    .product-grid .card-gallery .product-media-container picture,
    .product-grid .card-gallery .product-media-container video {
      width: 324px;
      height: 324px;
      object-fit: cover;
    }
  }

  /* Mobile sticky bottom bar */
  .collection-mobile-sticky-bar {
    display: none;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 100;
    background-color: #F8F3ED;
    border-top: 0.5px solid #D4D4D4;
  }

  @media screen and (max-width: 749px) {
    .collection-mobile-sticky-bar {
      display: flex;
      align-items: center;
      gap: 0;
    }
    
    /* Add padding to bottom of collection to account for sticky bar */
    results-list {
      padding-bottom: 80px;
    }
  }

  .collection-mobile-sticky-bar__divider {
    width: 1px;
    height: 40px;
    background-color: #CDCDCD;
    margin: 0 8px;
  }

  .collection-mobile-sticky-bar__button {
    flex: 1;
    display: flex;
    align-items: center;
    gap: 8px;
    padding: 12px 16px;
    background-color: transparent;
    border: none;
    cursor: pointer;
    font-family: 'Outfit', sans-serif;
    transition: background-color 0.2s ease;
  }

  .collection-mobile-sticky-bar__button:active {
    background-color: rgba(29, 66, 90, 0.05);
  }

  /* Active state when modal is open */
  .collection-mobile-sticky-bar__button--active {
    background-color: rgba(29, 66, 90, 0.1);
  }

  .collection-mobile-sticky-bar__icon {
    flex-shrink: 0;
    width: 20px;
    height: 20px;
  }

  .collection-mobile-sticky-bar__content {
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    flex: 1;
    min-width: 0;
  }

  .collection-mobile-sticky-bar__label {
   
      font-family: 'Outfit';
      font-style: normal;
      font-weight: 500;
      font-size: 16px;
      line-height: 140%;
      /* or 22px */

      color: #1D425A;



  }

  .collection-mobile-sticky-bar__value {

    font-family: 'Outfit';
    font-style: normal;
    font-weight: 400;
    font-size: 14px;
    line-height: 140%;

    color: #A5A2A2;


  }

  /* Mobile Sort Modal - matches filter drawer styling */
  .mobile-sort-modal__dialog {
    background-color: var(--color-background);
    border: none;
    padding: 0;
    max-width: 100%;
    width: 100%;
    max-height: 100vh;
    height: 100vh;
    margin: 0;
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: auto;
  }

  @media screen and (min-width: 750px) {
    .mobile-sort-modal__dialog {
      display: none;
    }
  }

  .mobile-sort-modal__form-wrapper {
    flex: 1 1 auto;
    min-height: 0;
    display: flex;
    flex-direction: column;
    overflow: hidden;
    height: 100%;
  }

  .mobile-sort-modal__form {
    flex: 1 1 auto;
    min-height: 0;
    display: flex;
    flex-direction: column;
    overflow: hidden;
    height: 100%;
  }

  .mobile-sort-modal__title-wrapper {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: var(--padding-md);
    border-bottom: 1px #CECACA;
    flex-shrink: 0;
  }

  .mobile-sort-modal__title {
    font-family: 'Outfit', sans-serif;
    font-style: normal;
    font-weight: 600;
    font-size: 18px;
    line-height: 140%;
    color: var(--color-foreground);
    margin: 0;
    display: flex;
    align-items: center;
    gap: var(--gap-xs);
  }

  .mobile-sort-modal__content {
    flex: 1 1 auto;
    min-height: 0;
    overflow-y: auto;
    overflow-x: hidden;
    padding: var(--padding-md);
    position: relative;
  }

  .mobile-sort-modal__option {
    display: flex;
    align-items: center;
    padding: 16px 0;
    cursor: pointer;
    transition: background-color 0.2s ease;
  }

  

  .mobile-sort-modal__option:active {
    background-color: rgb(var(--color-foreground-rgb) / var(--opacity-10));
  }

  .mobile-sort-modal__option:last-child {
    border-bottom: none;
  }

  .mobile-sort-modal__radio {
    display: none;
  }

  .mobile-sort-modal__option-text {
    font-family: 'Outfit', sans-serif;
    font-style: normal;
    font-weight: 400;
    font-size: 16px;
    line-height: 140%;
    color: #919191;
    display: block;
    transition: font-weight 0.2s ease, color 0.2s ease;
    flex: 1;
  }

  .mobile-sort-modal__option-text--selected {
 

      font-family: 'Outfit';
      font-style: normal;
      font-weight: 400;
      font-size: 16px;
      line-height: 140%;
      color: #1D425A;




  }

  .mobile-sort-modal__actions {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: var(--padding-md);
    gap: 24px;
    flex-shrink: 0;
    background-color: var(--color-background);
  }

  .mobile-sort-modal__button {
    background: transparent;
    border: none;
    /* Reset */



    font-family: 'Outfit';
    font-style: normal;
    font-weight: 400;
    font-size: 16px;
    line-height: 140%;
    /* or 22px */

    text-decoration-line: underline;

    color: #A5A2A2;




  }

  .mobile-sort-modal__button--apply {
    color: #1D425A;
  }

  .mobile-sort-modal__button--reset {
    color: #999999;
  }

  .mobile-sort-modal__button:active {
    opacity: 0.7;
  }

  @media screen and (min-width: 750px) {
    .mobile-sort-modal__dialog {
      display: none;
    }
  }
{% endstylesheet %}

{% schema %}
{
  "name": "t:names.collection_container",
  "enabled_on": {
    "templates": ["collection"]
  },
  "settings": [
    {
      "type": "select",
      "id": "layout_type",
      "label": "t:settings.type",
      "options": [
        {
          "value": "grid",
          "label": "t:options.grid"
        },
        {
          "value": "organic",
          "label": "t:options.editorial"
        }
      ],
      "default": "grid"
    },
    {
      "type": "select",
      "id": "product_card_size",
      "label": "t:settings.card_size",
      "options": [
        {
          "value": "small",
          "label": "t:options.small"
        },
        {
          "value": "medium",
          "label": "t:options.medium"
        },
        {
          "value": "large",
          "label": "t:options.large"
        },
        {
          "value": "extra-large",
          "label": "t:options.extra_large"
        }
      ],
      "default": "medium",
      "visible_if": "{{ section.settings.layout_type == 'grid' }}"
    },
    {
      "type": "select",
      "id": "mobile_product_card_size",
      "label": "t:settings.mobile_card_size",
      "options": [
        {
          "value": "small",
          "label": "t:options.small"
        },
        {
          "value": "large",
          "label": "t:options.large"
        }
      ],
      "default": "small"
    },
    {
      "type": "checkbox",
      "id": "enable_infinite_scroll",
      "label": "t:settings.auto_load_products",
      "default": true
    },
    {
      "type": "range",
      "id": "products_per_page",
      "label": "t:settings.products_per_page",
      "min": 8,
      "max": 36,
      "step": 4,
      "default": 24,
      "visible_if": "{{ section.settings.enable_infinite_scroll == false }}"
    },
    {
      "type": "header",
      "content": "t:content.layout"
    },
    {
      "type": "select",
      "id": "product_grid_width",
      "label": "t:settings.width",
      "options": [
        {
          "value": "centered",
          "label": "t:options.page"
        },
        {
          "value": "full-width",
          "label": "t:options.full"
        }
      ],
      "default": "centered"
    },
    {
      "type": "checkbox",
      "id": "full_width_on_mobile",
      "label": "t:settings.full_width_on_mobile",
      "default": true,
      "visible_if": "{{ section.settings.product_grid_width != 'full-width' }}"
    },
    {
      "type": "range",
      "id": "columns_gap_horizontal",
      "label": "t:settings.horizontal_gap",
      "min": 0,
      "max": 50,
      "step": 1,
      "unit": "px",
      "default": 16
    },
    {
      "type": "range",
      "id": "columns_gap_vertical",
      "label": "t:settings.vertical_gap",
      "min": 0,
      "max": 50,
      "step": 1,
      "unit": "px",
      "default": 16
    },
    {
      "type": "range",
      "id": "padding-inline-start",
      "label": "t:settings.left_padding",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "default": 0
    },
    {
      "type": "range",
      "id": "padding-inline-end",
      "label": "t:settings.right_padding",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "default": 0
    },
    {
      "type": "header",
      "content": "t:content.section_layout"
    },
    {
      "type": "color_scheme",
      "id": "color_scheme",
      "label": "t:settings.color_scheme",
      "default": "scheme-1"
    },
    {
      "type": "range",
      "id": "padding-block-start",
      "label": "t:settings.top_padding",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "default": 8
    },
    {
      "type": "range",
      "id": "padding-block-end",
      "label": "t:settings.bottom_padding",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "default": 8
    }
  ],
  "presets": []
}
{% endschema %}
